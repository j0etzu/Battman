	<true/>
	<key>com.apple.hid.manager.user-access-protected</key>
	<true/>

	<!-- PMGR/GG/SMC -->
	<key>com.apple.private.gasgauge-update</key>
	<true/>
	<key>com.apple.private.powersource-read</key>
	<true/>
	<key>com.apple.private.applesmc.user-access</key>
	<true/>
	<key>com.apple.gasgauge.user-access-device</key>
	<true/>

	<!-- Power/CoreDuet/LowPowerMode -->
	<key>com.apple.coreduetd.allow</key>
	<true/>
	<key>com.apple.coreduetd.batterysaver.allow</key>
	<true/>
	<key>com.apple.powerd.lowpowermode.allow</key>
	<true/>
	<key>com.apple.powerui.smartcharging</key>
	<true/>

